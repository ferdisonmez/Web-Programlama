@model List<User>

    <style>
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }
        td, th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }


        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>


    <h2>Kullanıcılar</h2>


    <table>

        <tr>
            <td>Name</td>
            <td>Surname</td>
            <td>Email</td>
        </tr>
        @{ foreach (var temp in Model)
            {

                <tr>
                    <td>@temp.Name</td>
                    <td>@temp.Surname</td>
                    <td>@temp.Email</td>
                </tr>

            }
        }

    </table>


